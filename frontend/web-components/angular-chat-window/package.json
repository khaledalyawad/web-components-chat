{
  "name": "angular-chat-window",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "wc": "npm run wc:clean && npm run wc:copy",
    "wc:clean": "rm -rf src/assets/web-components && mkdir src/assets/web-components",
    "wc:copy": "cp ../../dist/lit-element-chat-link.js src/assets/web-components",
    "build": "ng build",
    "build-wc": "ng build --prod && npm run build-wc:bundle",
    "build-wc:bundle": "cat dist/angular-chat-window/runtime-es2015.*.js > dist/angular-chat-window.js && cat dist/angular-chat-window/polyfills-es2015.*.js >> dist/angular-chat-window.js && cat dist/angular-chat-window/main-es2015.*.js >> dist/angular-chat-window.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/elements": "^8.2.6",
    "@angular/forms": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "document-register-element": "^1.7.2",
    "luxon": "^1.17.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/luxon": "^1.15.2",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
